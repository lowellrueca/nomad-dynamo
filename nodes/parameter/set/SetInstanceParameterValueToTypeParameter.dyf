  <Elements>
    <PythonNodeModels.PythonNode guid="b89ccfad-5642-40bf-9aa1-41f0660ea02b" type="PythonNodeModels.PythonNode" nickname="Python Script" x="611.2" y="429.6" isVisible="true" isUpstreamVisible="true" lacing="Disabled" isSelectedInput="False" IsFrozen="false" isPinned="false" inputcount="3">
      <PortInfo index="0" default="False" />
      <PortInfo index="1" default="False" />
      <PortInfo index="2" default="False" />
      <Script>
import clr

clr.AddReference('RevitAPI')
clr.AddReference('RevitServices')

from Autodesk.Revit.DB import *
from RevitServices.Persistence import DocumentManager as DM
from RevitServices.Transactions import TransactionManager as TM
from System.Collections.Generic import List


document = DM.Instance.CurrentDBDocument
elements = UnwrapElement(IN[0])
inst_parameter_name = IN[1]
type_parameter_name = IN[2]
res = []

inst_parameter = map(lambda element: element.LookupParameter(inst_parameter_name), elements)[0]

inst_parameter_value = inst_parameter.AsString()

element_ids = List[ElementId](map(lambda element: element.GetTypeId(), elements))

element_types = FilteredElementCollector(document, element_ids).WhereElementIsElementType().ToElements()

try:
	TM.Instance.EnsureInTransaction(doc)

	res = map(lambda element_type: element_type.LookupParameter(type_parameter_name).Set(inst_parameter_value), element_types)
	
except:
	TM.Instance.ForceCloseTransaction()
	
finally:
	TM.Instance.TransactionTaskDone()

OUT = element_types, res
</Script>
